<?xml version="1.0" encoding="utf-8"?>
<com.github.evan.common_utils.ui.view.nestingTouchView.NestingHorizontalScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:horizontal_scroll_view_intercept_mode="horizontal_by_itself">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

        <com.github.evan.common_utils.ui.view.nestingTouchView.NestingScrollView
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            app:scroll_view_intercept_mode="all_by_itself">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="" />
        </com.github.evan.common_utils.ui.view.nestingTouchView.NestingScrollView>


        <com.github.evan.common_utils.ui.view.nestingTouchView.NestingScrollView
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            app:scroll_view_intercept_mode="all_by_itself">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="" />

        </com.github.evan.common_utils.ui.view.nestingTouchView.NestingScrollView>


        <com.github.evan.common_utils.ui.view.nestingTouchView.NestingScrollView
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            app:scroll_view_intercept_mode="all_by_itself">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="" />

        </com.github.evan.common_utils.ui.view.nestingTouchView.NestingScrollView>


        <com.github.evan.common_utils.ui.view.nestingTouchView.NestingScrollView
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            app:scroll_view_intercept_mode="all_by_itself">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="" />

        </com.github.evan.common_utils.ui.view.nestingTouchView.NestingScrollView>


    </LinearLayout>


</com.github.evan.common_utils.ui.view.nestingTouchView.NestingHorizontalScrollView>